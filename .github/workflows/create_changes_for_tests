name: Create Changes for Test

on:
  push:
    brances: [ main ]
  pull_request:
    brances: [ main ]

jobs:
  build:
    strategy:
      matrix: 
        platform: [ubuntu-latest, macos-latest, windows-latest]
    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v2
    - name: Set up Python 3.9
      uses: actions/setup-python@v2
      with:
        python-version: 3.9
    - name: Give changes
      run: |
          echo "" > a.txt
          rm -rf a.txt
          echo "" > a.txt
          rm -rf a.txt
